Algoritmo TipodeCambioPesoaDolar
	//Definir variables
	Definir cantPesos, tipoCambio, cantDolar como real
	//Datos de entrada
	Escribir "CANTIDAD DE PESOS QUE TIENE:"
	Leer cantPesos
	Escribir "INGRESE EL TIPO DE CAMBIO:"
	Leer tipoCambio
	//Proceso
	cantDolar<-(cantPesos*tipoCambio)
	//Datos de salida
	Escribir "EL MONTO A OBTENER ES:", cantDolar, "DOLARES"
FinAlgoritmo
